# Recursive Fibonacci Function
def fib_rec(n):
    if n <= 1:
        return n
    return fib_rec(n-1) + fib_rec(n-2)

# Non-Recursive Fibonacci Function
def fib_non_rec(n):
    a, b = 0, 1
    for _ in range(n):
        print(a, end=" ")
        a, b = b, a + b

n = int(input("Enter n: "))

print("\nRecursive Fibonacci:")
for i in range(n):
    print(fib_rec(i), end=" ")

print("\n\nNon-Recursive Fibonacci:")
fib_non_rec(n)
print()
